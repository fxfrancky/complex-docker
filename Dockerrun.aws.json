{
"AWSEBDockerrunVersion":2,
"containerDefinitions": [
    {
        "memoryReservation" : 256,
        "name": "client",
        "image": "fxfrancky/multi-client",
        "hostname": "client",
        "essential": false
    },
    {
        "memoryReservation" : 256,
        "name": "server",
        "image": "fxfrancky/multi-server",
        "hostname": "api",
        "essential": false
    },
    {
        "memoryReservation" : 256,
        "name": "worker",
        "image": "fxfrancky/multi-worker",
        "hostname": "worker",
        "essential": false
    },
    {
        "memoryReservation" : 256,
        "name": "nginx",
        "image":"fxfrancky/multi-nginx",
        "hostname":"nginx",
        "essential":true,
        "portMappings": [
            {
                "hostPort": 80,
                "containerPort": 80
            }

        ],
        "links": ["client","server"]
    }
]

}



